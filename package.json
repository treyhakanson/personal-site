{
  "name": "personal-site",
  "version": "0.0.0",
  "description": "My personal site, an ongoing projects",
  "main": "index.js",
  "scripts": {
    "start": "NODE_ENV=development nodemon api/index.js --exec babel-node",
    "dev": "webpack-dev-server --config ./webpack.dev.config.js --define process.env.NODE_ENV=\"'development'\" --hot",
