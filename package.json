    "npm": "3.10.10"
  },
  "dependencies": {
    "axios": "^0.16.2",
    "body-parser": "^1.17.2",
    "ejs": "^2.5.7",
    "express": "^4.15.4",
    "highlight.js": "^9.12.0",
    "moment": "^2.18.1",
    "pg-promise": "^6.5.2",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-redux": "^4.4.1",
    "react-router": "^2.0.0",
    "react-svg-loader": "^1.1.1",
    "redux": "^3.3.1"
  },
